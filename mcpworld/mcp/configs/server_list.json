{
  "weather": {
    "stdio": {
      "command": "python3",
      "args": [
        "-m", "mcpworld.mcp.servers.weather"
      ]
    },
    "sse": {
      "command": "python3",
      "args": [
        "-m", "mcpworld.mcp.servers.weather",
        "--transport", "sse",
        "--port", "{{PORT}}"
      ]
    }
  },

  "echo": {
    "stdio": {
      "command": "python3",
      "args": [
        "-m", "mcpworld.mcp.servers.echo"
      ]
    },
    "sse": {
      "command": "python3",
      "args": [
        "-m", "mcpworld.mcp.servers.echo",
        "--transport", "sse",
        "--port", "{{PORT}}"
      ]
    }
  },

  "google-search": {
    "stdio": {
      "command": "python3",
      "args": [
        "-m", "mcpworld.mcp.servers.google_search"
      ]
    },
    "sse": {
      "command": "python3",
      "args": [
        "-m", "mcpworld.mcp.servers.google_search",
        "--transport", "sse",
        "--port", "{{PORT}}"
      ]
    }
  },

  "wikipedia": {
    "stdio": {
      "command": "python3",
      "args": [
        "-m", "mcpworld.mcp.servers.wikipedia"
      ]
    },
    "sse": {
      "command": "python3",
      "args": [
        "-m", "mcpworld.mcp.servers.wikipedia",
        "--transport", "sse",
        "--port", "{{PORT}}"
      ]
    }
  },

  "google-maps": {
    "stdio": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-google-maps"
      ]
    },
    "env": {
      "GOOGLE_MAPS_API_KEY": "{{GOOGLE_MAPS_API_KEY}}"
    }
  },

  "fetch": {
    "stdio": {
      "command": "python3",
      "args": [
        "-m", "mcp_server_fetch",
        "--ignore-robots-txt"
      ]
    }
  },

  "github": {
    "stdio": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ]
    },
    "env": {
      "GITHUB_PERSONAL_ACCESS_TOKEN": "{{GITHUB_PERSONAL_ACCESS_TOKEN}}"
    }
  },

  "google-sheets": {
    "stdio": {
      "command": "python3",
      "args": [
        "-m", "mcpworld.mcp.servers.google_sheets"
      ]
    },
    "sse": {
      "command": "python3",
      "args": [
        "-m", "mcpworld.mcp.servers.google_sheets",
        "--transport", "sse",
        "--port", "{{PORT}}"
      ]
    },
    "env": {
      "SERVICE_ACCOUNT_PATH": "{{GOOGLE_SERVICE_ACCOUNT_PATH}}",
      "DRIVE_FOLDER_ID": "{{GOOGLE_DRIVE_FOLDER_ID}}"
    }
  },

  "filesystem": {
    "stdio": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "{{FILESYSTEM_DIRECTORY}}"
      ]
    }
  },

  "slack": {
    "stdio": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-slack"
      ]
    },
    "env": {
      "SLACK_BOT_TOKEN": "{{SLACK_BOT_TOKEN}}",
      "SLACK_TEAM_ID": "{{SLACK_TEAM_ID}}"
    }
  },

  "postgres": {
    "stdio": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "{{POSTGRES_ADDRESS}}"
      ]
    }
  },

  "calculator": {
    "stdio": {
      "command": "python3",
      "args": [
        "-m", "mcp_server_calculator"
      ]
    }
  },

  "blender": {
    "stdio": {
      "command": "python3",
      "args": [
        "-m", "blender_mcp.server"
      ]
    }
  },

  "notion": {
    "stdio": {
      "command": "npx",
      "args": [
        "-y", "@notionhq/notion-mcp-server"
      ]
    },
    "env": {
      "OPENAPI_MCP_HEADERS": "{\"Authorization\": \"Bearer {{NOTION_API_KEY}}\", \"Notion-Version\": \"2022-06-28\" }"
    }
  }
}
